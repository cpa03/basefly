{
  "name": "@saasfly/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "contentlayer2 build && npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- next build",
    "build:analyze": "ANALYZE=true contentlayer2 build && npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- next dev",
    "lint": "npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- eslint . --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "format": "prettier --write '**/*.{js,cjs,mjs,ts,tsx,md,json}'  --ignore-path .prettierignore",
    "start": "npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- next start",
    "with-env": "echo 'Use individual scripts with dotenv-cli prefix instead'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.5",
    "@dinero.js/currencies": "2.0.0-alpha.15",
    "@formatjs/intl-localematcher": "0.8.1",
