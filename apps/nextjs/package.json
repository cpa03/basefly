{
  "name": "@saasfly/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "contentlayer2 build && npx dotenv-cli -e ../../.env.local -- next build",
    "build:analyze": "ANALYZE=true contentlayer2 build && npx dotenv-cli -e ../../.env.local -- next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "npx dotenv-cli -e ../../.env.local -- next dev",
    "lint": "npx dotenv-cli -e ../../.env.local -e ../../.env.ci -- eslint . --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "format": "prettier --write '**/*.{js,cjs,mjs,ts,tsx,md,json}'  --ignore-path .prettierignore",
    "start": "npx dotenv-cli -e ../../.env.local -- next start",
    "with-env": "echo 'Use individual scripts with dotenv-cli prefix instead'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.6",
    "@dinero.js/currencies": "2.0.0-alpha.14",
    "@formatjs/intl-localematcher": "0.5.4",
    "@hookform/resolvers": "3.3.4",
    "@next/mdx": "14.2.5",
    "@saasfly/api": "workspace:*",
    "@saasfly/auth": "workspace:*",
    "@saasfly/common": "workspace:*",
    "@saasfly/db": "workspace:*",
    "@saasfly/stripe": "workspace:*",
    "@saasfly/ui": "workspace:*",
    "@t3-oss/env-nextjs": "0.8.0",
    "@tanstack/react-query": "5.36.0",
    "@tanstack/react-query-devtools": "5.36.0",
    "@tanstack/react-table": "8.17.0",
    "@trpc/client": "10.45.3",
    "@trpc/next": "10.45.3",
    "@trpc/react-query": "10.45.3",
    "@trpc/server": "10.45.3",
    "@vercel/analytics": "1.1.3",
    "@vercel/speed-insights": "1.0.9",
    "date-fns": "3.3.1",
    "fetch-ponyfill": "7.1.0",
    "framer-motion": "11.0.3",
    "lucide-react": "0.323.0",
    "negotiator": "0.6.3",
    "next": "14.2.35",
    "next-themes": "0.2.1",
    "posthog-js": "^1.266.0",
    "posthog-node": "^4.0.0",
    "react": "18.3.1",
    "react-day-picker": "8.10.0",
    "react-dom": "18.3.1",
