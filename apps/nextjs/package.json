    "@trpc/react-query": "11.10.0",
    "@trpc/server": "11.10.0",
    "@vercel/analytics": "1.6.1",
    "@vercel/speed-insights": "1.3.1",
    "date-fns": "4.1.0",
    "fetch-ponyfill": "7.1.0",
    "framer-motion": "12.34.2",
    "lucide-react": "0.574.0",
    "negotiator": "1.0.0",
    "next": "14.2.35",
