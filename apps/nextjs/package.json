    "prettier": "3.7.4",
    "puppeteer": "^24.37.2",
    "rehype": "13.0.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-pretty-code": "0.12.3",
    "rehype-slug": "6.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.9.3",
    "unist-util-visit": "5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@saasfly/eslint-config/base",
      "@saasfly/eslint-config/nextjs",
      "@saasfly/eslint-config/react"
    ]
  },
  "prettier": "@saasfly/prettier-config"
