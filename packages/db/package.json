{
  "name": "@saasfly/db",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": "./index.ts"
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "format": "prisma format && prettier --check '**/*.{ts,json}' ",
    "db:generate": "prisma generate",
    "db:push": "bun with-env prisma db push --skip-generate",
    "db:migrate:dev": "bun with-env prisma migrate dev",
    "db:migrate:deploy": "bun with-env prisma migrate deploy",
    "db:migrate:create": "bun with-env prisma migrate dev --create-only",
    "db:migrate:reset": "bun with-env prisma migrate reset",
    "db:migrate:resolve": "bun with-env prisma migrate resolve",
    "db:studio": "bun with-env prisma studio",
    "db:seed": "bun with-env tsx seed.ts",
    "db:seed:reset": "bun with-env tsx -e \"import('./seed').then(m => m.clearSeedData().then(() => m.seed()))\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "kysely": "0.28.11",
    "@vercel/postgres-kysely": "0.10.0"
  },
  "devDependencies": {
    "@saasfly/eslint-config": "workspace:*",
    "@saasfly/prettier-config": "workspace:*",
    "@saasfly/typescript-config": "workspace:*",
    "dotenv-cli": "7.3.0",
    "eslint": "8.57.0",
    "prettier": "3.8.1",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "eslint-plugin-import": "2.32.0",
    "prisma": "5.9.1",
    "prisma-kysely": "1.7.1",
    "@types/pg": "8.11.0",
    "tsx": "^4.19.0",
    "typescript": "5.9.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@saasfly/eslint-config/base"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  },
  "prettier": "@saasfly/prettier-config"
}
